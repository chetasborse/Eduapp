 all_subjects = this.state.subjects.map((subject) => (\n      <option\n        key={subject.subject_id}\n        value={subject.subject_name}\n        id={subject.subject_id}\n      >\n        {subject.subject_name}\n      </option>\n    ));\n\n    var all_languages = this.state.languages.map((language) => (\n      <option\n        key={language.language_id}\n        value={language.language_name}\n        id={language.language_id}\n      >\n        {language.language_name}\n      </option>\n    ));\n\n    return (\n      <Container className=\"toplookout\">\n        <Row>\n          <Col>\n            <h3>Personal Details</h3>\n            {!this.state.edit_profile && (\n              <React.Fragment>\n                <p>Username: {username}</p>\n                <p>\n                  Name: {first_name} {last_name}\n                </p>\n                <p>Email: {email_id}</p>\n                {/* <p>Image Link: {image_link}</p> */}\n                <img src={this.props.image_link} className=\"profilepic\"></img>\n                <p>Sessions taken: {session_taken}</p>\n                {is_teacher && (\n                  <React.Fragment>\n                    <p>Qualification: {qualification}</p>\n                    <p>Rating Points: {rating_points}</p>\n                  </React.Fragment>\n                )}\n                {!is_teacher && (\n                  <React.Fragment>\n                    <p>Grade: {grade}</p>\n                    <p>Board: {board}</p>\n                  </React.Fragment>\n                )}\n                <Button onClick={this.toggle_profile}>Edit Profile</Button>\n              </React.Fragment>\n            )}\n            {this.state.edit_profile && (\n              <React.Fragment>\n                <Form>\n                  <FormGroup row>\n                    <Label for=\"first_name\" sm={2}>\n                      First Name:\n                    </Label>\n                    <Col sm={10}>\n                      <Input\n        