ndle_file}\n                      ></Input>\n                    </Col>\n                  </FormGroup>\n                  {this.props.is_teacher && (\n                    <FormGroup row>\n                      <Label for=\"qualification\" sm={2}>\n                        Qualification:\n                      </Label>\n                      <Col sm={10}>\n                        <Input\n                          type=\"text\"\n                          name=\"qualification\"\n                          id=\"qualification\"\n                          placeholder=\"Enter your Qualification\"\n                          value={this.state.qualification}\n                          onChange={this.handle}\n                        ></Input>\n                      </Col>\n                    </FormGroup>\n                  )}\n                  {!this.props.is_teacher && (\n                    <React.Fragment>\n                      <FormGroup row>\n                        <Label for=\"grade\" sm={2}>\n                          Grade:\n                        </Label>\n                        <Col sm={10}>\n                          <Input\n                            type=\"number\"\n                            name=\"grade\"\n                            id=\"grade\"\n                            placeholder=\"Enter your Grade\"\n                            value={this.state.grade}\n                            onChange={this.handle}\n                          ></Input>\n                        </Col>\n                      </FormGroup>\n                      <FormGroup row>\n                        <Label for=\"board\" sm={2}>\n                          Board:\n                        </Label>\n                        <Col sm={10}>\n                          <Input\n                            type=\"select\"\n                            name=\"board\"\n                            id=\"board\"\n                            placeholder=\"Enter your board\"\n                     